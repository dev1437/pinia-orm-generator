{
    "name": "dev1437/pinia-model-generator",
    "description": "Generate Pinia ORM models based on your Laravel models",
    "autoload": {
        "psr-4": {
            "Dev1437\\PiniaModelGenerator\\": "src/"
        }
    },
    "require": {
        "dev1437/model-parser": "^1.0",
        "doctrine/dbal": "^3.4",
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dev1437\\PiniaModelGenerator\\PiniaModelGeneratorServiceProvider"
            ]
        }
    }
}
